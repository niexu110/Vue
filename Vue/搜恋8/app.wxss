page{font-family: 'PingFang Medium','Arial Narrow Bold', sans-serif}
/*登录图片/注册图片*/
.bannerMsg{
    width:100%;
    height:196rpx;
}
.bannerMsg image{
    display: block;
    width:100%;
    height:100%;
}
.lt{float:left;}
.rt{float:right;}
/*登录表单/注册表单*/
.loginForm{
    display: block;
    width:670rpx;
    margin:0 auto;
    padding-top:40rpx;
}
.section{
    width:100%;
    display: flex;
    flex-flow:row nowrap;
    justify-content:flex-start;
    margin-bottom:40rpx;
    border:1px solid #dddddd;
    padding:0;
    
}
.section image{
    width:92rpx;
    height:92rpx;
    background: #dddddd;
}
.section input{
    flex:1;
    height:90rpx;
    line-height:90rpx;
    margin-left:5rpx;
    padding-left:15rpx;
    color:#5793dd;
}
.placeStyle{
    font-size:30rpx;
    color:#cccccc;
}
/*登录按钮/注册按钮*/
.loginForm button{
    width:100%;
    height:90rpx;
    background-color: #5793dd;
    font-size: 40rpx;
    color:#fff;
    margin-bottom:40rpx;
}