/* pages/thirdStepDate/thirdStepDate.wxss */
.title{
    height: 70rpx;
    padding: 0 40rpx;
    background: #fff;
    line-height: 70rpx;
    font-size: 26rpx;
    color:#999;
}
.empty{
    height:20rpx;
    background: #f9f9f9;
}
.date_content{
    padding: 10rpx 40rpx;
    border-bottom: solid 1px #e6e6e6
}
.dateHeart{
    display: block;
    font-size: 36rpx;
    color:#666;
    line-height: 60rpx;
}
.dataState{
    font-size: 24rpx;
    color: #5793dd;
    line-height: 58rpx;
}
.dateSay{
    width: 670rpx;
    height:176rpx;
    border: solid 1px #666;
    border-radius:4px;
    font-size: 32rpx;
    color:#666;
}
.dataState_text{
    position: relative;
    font-size: 28rpx;
    color: #5793dd;
}
.date_contenttext{
    font-size: 28rpx;
    color: #5793dd;
}
/*计数*/
.count{
    position: absolute;
    right: 20rpx;
    bottom: 20rpx;
    color: #999;
    font-size: 36rpx;
}
.radio_group{
    display: flex;
    flex-direction: column;
    line-height: 60rpx;
    font-size: 22rpx;
    color: #999;
}
.state_content{
    padding: 0 40rpx;
    height:130rpx;
    line-height: 90rpx;
    font-size: 26rpx;
    text-align: center;
    color: #feb3c5;
}
.content_bottom{
    margin: 0 40rpx;
    height:90rpx;
    line-height: 90rpx;
    text-align: center;
    font-size: 36rpx;
    color:#fff;
    background: #feb3c5;
    border-radius:2px;
}
/*按键设置*/
.date1{
    font-size: 24rpx;
    color:#999;
    line-height: 60rpx;
}
.date1_1{
    display: flex;
    align-items: center;
}
.key_wai,.key_wai1{
    height:34rpx;
    width:34rpx;
    background: #feb3c5;
    display: flex;
    justify-content: center;
    align-items: center;
}
.key_zhong,.key_nei2{
    height:26rpx;
    width:26rpx;
    background: #fff;
    display: flex;
    justify-content: center;
    align-items: center;
}
.key_nei,.key_nei1{
    height:18rpx;
    width:18rpx;
    background: #feb3c5;
}
.key_wai2,.key_wai21{
    height:34rpx;
    width:34rpx;
    background: #9c9c9c;
    display: flex;
    justify-content: center;
    align-items: center;
}